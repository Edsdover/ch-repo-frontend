.row
  h4 Admin Accounts
  .col-md-4.col-xs-11.userCard(ng-repeat='admin in admins')
    .col-xs-4
      img.img-responsive.img-thumbnail.userListPic(ng-src='{{admin.profileImageURL}}')
    .col-xs-8
      h5 Name: {{admin.displayName}}
      h5 Username: {{admin.username}}
      h5.cursor(ng-click='toggleAdminOff()') Admin: {{admin.adminUser}}
        .glyphicon.glyphicon-arrow-down
.row
  h4 Student Accoutns
  .col-md-4.col-xs-11.userCard(ng-repeat='student in students')
    .col-xs-4
      img.img-responsive.img-thumbnail.userListPic(ng-src='{{student.profileImageURL}}')
    .col-xs-8
      h5 Name: {{student.displayName}}
      h5 Username: {{student.username}}
      h5.cursor(ng-click='toggleAdminOn()') Admin: {{student.adminUser}}
        .glyphicon.glyphicon-arrow-up
